CREATE DATABASE IF EXIST ejercicio1;
DROP DATABASE ejercicio1;
USE ejercicio1;

CREATE TABLE Producto (
    nombre CHAR(10),
    pais VARCHAR(50),
    cantidad INT,
    precio FLOAT,
    peso_gramos DECIMAL(5, 2),
    fecha_creacion DATE,
    fecha_caducidad DATETIME,
    fecha_entrega TIME
);

